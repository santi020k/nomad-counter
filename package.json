{
  "name": "nomad-counter",
  "type": "module",
  "version": "0.5.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "pre-commit": "lint-staged && vitest run",
    "pre-push": "eslint . --fix --max-warnings=0 && npm run build",
    "prepare": "husky config/.husky",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --coverage.enabled=true --ui --watch"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.1.0",
    "@astrojs/react": "^3.0.9",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/vercel": "^7.0.2",
    "@fontsource-variable/cabin": "^5.0.17",
    "@supabase/supabase-js": "^2.39.3",
    "@tabler/icons-react": "^2.46.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vercel/analytics": "^1.1.2",
    "@vitejs/plugin-react": "^4.2.1",
    "astro": "^4.2.6",
    "astro-i18n-aut": "^0.6.5",
    "dayjs": "^1.11.10",
    "i18next": "23.8.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-chained-backend": "^4.6.2",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.2",
    "preline": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-tailwindcss-datepicker": "^1.6.6",
    "tailwindcss": "^3.4.1",
    "toastify-js": "^1.12.0",
    "typescript": "^5.3.3",
    "usehooks-ts": "^2.10.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.14.202",
    "@types/toastify-js": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^4.6.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-astro": "^0.31.3",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.20",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "vitest": "^1.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro,md}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
